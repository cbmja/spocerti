<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.spo.certificate.exam.mapper.ExamDataMapper">

    <!--insert-->
    <insert id="save" parameterType="ExamData">
        INSERT INTO exam_data (year , type , exam_id , path , exam_title , file_name)
        VALUES(#{year} , #{type} , #{examId} , #{path} , #{examTitle} , #{fileName})
    </insert>
    <!--insert-->


    <!--select-->
    <select id="findById"  parameterType="int" resultType="ExamData">
        SELECT * FROM exam_data WHERE id = #{id}
    </select>

    <select id="findAll" resultType="ExamData">
        SELECT * FROM exam_data
    </select>

    <select id="findByYearAndExamId" parameterType="MainSearch" resultType="ExamData">
        SELECT * FROM exam_data where year = #{year} AND exam_id = #{examId} ORDER BY year asc
    </select>
    <!--select-->

</mapper>