<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.spo.certificate.exam.mapper.ExamDataMapper">

    <!--insert-->
    <insert id="createExamData" parameterType="ExamData">
        INSERT INTO exam_data (id , year , type , exam_id , path , exam_title , file_name)
        VALUES(#{id} , #{year} , #{type} , #{examId} , #{path} , #{examTitle} , #{fileName})
    </insert>
    <!--insert-->


    <!--select-->
    <select id="findById"  parameterType="int" resultType="Subject">
        SELECT * FROM subject WHERE id = #{id}
    </select>

    <select id="findAll" resultType="Subject">
        SELECT * FROM subject
    </select>
    <!--select-->

</mapper>