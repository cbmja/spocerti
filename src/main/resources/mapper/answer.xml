<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.spo.certificate.test.answer.mapper.AnswerMapper">

    <!--select-->

    <select id="findByExamCodeAndSubjectCodeAndYearAndType" parameterType="Answer" resultType="Answer">
        SELECT * FROM answer WHERE exam_code = #{examCode} AND subject_code = #{subjectCode} AND year = #{year} AND type = #{type}
    </select>

    <select id="findAll" resultType="Exam">
        SELECT * FROM answer
    </select>
    <!--select-->

</mapper>