<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.spo.certificate.test.test.mapper.TestDataMapper">
    <!--insert-->
    <insert id="save" parameterType="TestData">
        INSERT INTO spocerti_ver_2.test_data (user_code,exam_code,year,type,score,result)
        VALUES(#{userCode},#{examCode},#{year},#{type},#{score},#{result})
    </insert>
    <!--insert-->

    <!--select-->
    <select id="findAll" resultType="TestData">
        SELECT * FROM spocerti_ver_2.test_data
    </select>
    <select id="getCode" resultType="int">
        SELECT LAST_INSERT_ID();
    </select>
    <!--select-->

</mapper>